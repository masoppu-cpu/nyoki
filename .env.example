# Supabase設定（必須）
EXPO_PUBLIC_SUPABASE_URL=https://xxx.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=eyJxxxxx
SUPABASE_SERVICE_ROLE_KEY=eyJxxxxx

# Gemini API設定（必須）
GEMINI_API_KEY=your_gemini_api_key

# RevenueCat設定（Phase 3で必須）
EXPO_PUBLIC_REVENUECAT_API_KEY=your_revenuecat_key

# OneSignal設定（Phase 3で必須）
EXPO_PUBLIC_ONESIGNAL_APP_ID=your_onesignal_app_id

# Mixpanel設定（任意）
EXPO_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token

# その他の設定
NODE_ENV=development