{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PUBLIC_USE_MOCK": "true"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_USE_MOCK": "false"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "<your-apple-id>",
        "ascAppId": "<your-appstore-connect-app-id>",
        "appleTeamId": "<your-team-id>"
      }
    }
  }
}
